let test = "Hello\n";

fn add(x: Int, y: Int): Int{
    return x + y;
};

fn calcAverage(x: Int, y: Int, z: Int): Int{
    let mut sum = x + y + z;
    return sum / 3;
};

fn main(argc: Int){
    let addResult: Int = add(10, 20);
    printf(test);
    printf("%d\n", addResult);
    printf("%d\n", calcAverage(5, 3, 6));
};