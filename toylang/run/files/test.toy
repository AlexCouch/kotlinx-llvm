fn add(x: Int, y: Int): Int{
    return x + y;
};

fn sub(x: Int, y: Int): Int{
    return x - y;
};

fn mul(x: Int, y: Int): Int{
    return x * y;
};

fn div(x: Int, y: Int): Int{
    return x / y;
};

fn average(x: Int, y: Int, z: Int): Int{
    let sum = add(x, add(y, z));
    return div(sum, 3);
};

fn main(args: Int): Int{
    print("Hello");
    print(average(5, 3, 4));
};